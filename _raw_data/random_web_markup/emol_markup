<script>

  function openFaroxPopup() {
   return; 
    if (document.cookie.indexOf('faroxAD') >= 0)
      return;
    
    /* set cookie */  
    var expires = new Date();
    expires.setTime(expires.getTime() + 86400 * 1000);
      
    document.cookie = "faroxAD=1; expires=" + expires.toGMTString() + "; path=/";
    
    /* popup params */
	// farox 400,500  fans facebook 420 620, economicos 450 700
    var width = 450
      , height = 700
  //    , left = screen.availWidth / 2 - width / 2
      , left = 0
      , top  = screen.availHeight / 2 - height / 2;
    /*
    var width = 500
      , height = 400
      , left = screen.availWidth / 2 - width / 2
      , top  = screen.availHeight / 2 - height / 2;
   */
    //var params = ['height=' + height, 'width=' + width, 'left=' + left, 'top=' + top, 'location=no,status=no,scrollbars=no,resizable=yes,titlebar=no,menubar=no'].join(',');
    var params = ['height=' + height, 'width=' + width, 'left=' + left, 'top=' + top, 'location=no,status=no,scrollbars=no,resizable=no,titlebar=no,menubar=no'].join(',');
    
    //window.open("http://www.guioteca.com/static/ads/farox/incognito.html", "_ppFarox", params);
    //window.open("http://www.guioteca.com/static/ads/farox/50lucas.html", "_ppFarox", params);
    //window.open("http://www.farox.com/modals/popup_guioteca", "_ppFarox", params);
    //window.open("http://www.vivedescuentos.com/index.php?fuseaction=publico.emailLocation&noLayout=1&origen=20&v=4&step=2", "_ppFarox", params);
    //window.open("http://www.guioteca.com/static/ads/farox/smartphone.html", "_ppFarox", params);
//    window.open("http://www.guioteca.com/static/ads/farox/giftcardnavidad.html", "_ppFarox", params);
//    window.open("http://www.guioteca.com/static/ads/farox/ipadmini.html", "_ppFarox", params);
//    window.open("http://www.guioteca.com/static/ads/guioteca/fans.html", "_ppFarox", params);
//    window.open("http://www.guioteca.com/static/ads/vive/galaxy.html", "_ppFarox", params);
