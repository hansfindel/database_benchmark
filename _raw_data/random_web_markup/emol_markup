		<!--script  language="javascript" type="text/javascript">
		//<![CDATA[ 

					var scrollTimeStart = new Date;
					var scrollEvent100=0;
					var scrollEvent90 = 0;
					var scrollEvent75 = 0;
					var scrollEvent50 = 0;
					var scrollEvent35 = 0;
					var scrollEvent20 = 0;
					 
					$(document).ready(function () {
								IniciarGoogleAnalyticsTrackEvents();
					});
					 
					function IniciarGoogleAnalyticsTrackEvents(){
								TrackingScroll();
					}
					 
					function TrackingScroll(){
								$(window).scroll(function(){
										   var scrollPercent = GetScrollPercent();
										   //fin home
										   if(scrollPercent==100 && scrollEvent100!=1){     
																   scrollEvent100=1;
																   var scrollTime = (new Date - scrollTimeStart )/1000;
																   var roundTimeScroll = Math.round(scrollTime,1);
																   TrackEvent("Portada-Emol", "Scroll en 100%", document.location.href, roundTimeScroll);
										   }
										   //fin cajas home
										   if(scrollPercent>90 && scrollEvent90!=1){          
																   scrollEvent90=1;
																   var scrollTime = (new Date - scrollTimeStart )/1000;
																   var roundTimeScroll = Math.round(scrollTime,1);
																   TrackEvent("Portada-Emol", "Scroll en 90%", document.location.href, roundTimeScroll);
										   }
										   //fin cajas de las secciones del sitio
										   if(scrollPercent>75 && scrollEvent75!=1){          
																   scrollEvent75=1;
																   var scrollTime = (new Date - scrollTimeStart )/1000;
																   var roundTimeScroll = Math.round(scrollTime,1);
																   TrackEvent("Portada-Emol", "Scroll en 75%", document.location.href, roundTimeScroll);
										   }
										   //al menos 2 cajas
										   if(scrollPercent>50 && scrollEvent50!=1){
																   scrollEvent50=1;
																   var scrollTime = (new Date - scrollTimeStart )/1000;
																   var roundTimeScroll = Math.round(scrollTime,1);
																   TrackEvent("Portada-Emol", "Scroll en 50%", document.location.href, roundTimeScroll);
										   }
										   //termino de la diagramación de noticias
										   if(scrollPercent>35 && scrollEvent35!=1){
																   scrollEvent35=1;
																   var scrollTime = (new Date - scrollTimeStart )/1000;
																   var roundTimeScroll = Math.round(scrollTime,1);
																   TrackEvent("Portada-Emol", "Scroll en 35%", document.location.href, roundTimeScroll);
										   }
										   //scroll activo
										   //if(scrollPercent>20 && scrollEvent20!=1){
											//					   scrollEvent20=1;
											//					   var scrollTime = (new Date - scrollTimeStart )/1000;
											//					   var roundTimeScroll = Math.round(scrollTime,1);
											//					   TrackEvent("Portada-Emol", "Scroll en 20%", document.location.href, roundTimeScroll);
										   //}
								});
					}
					 
					function GetScrollPercent(){
								var bottom = $(window).height() + $(window).scrollTop();
								var height = $(document).height();
								return Math.round(100*bottom/height);
					}
					 
					function TrackEvent(Category, Action, Label, Value){
								_gaq.push(['_trackEvent', Category, Action, Label, Value, true]);
					} 
				 //]]>    
		</script--> 
	<script language="javascript" type="text/javascript">
	//<![CDATA[ 
			var FechaModificacionHomePage="";		
			var HomePagepageCoords="";
			var HomePagepageCoordsActual="";
			var CheckFechaModificacionHomePage="";
			var ctrlslideremoltv=0;
			
			function CargaFechaModificacionHomePage(){
				var t = $.getJSON("/_portada/fecha_modificacion_json.asp",function(e){FechaModificacionHomePage = e.fechahome}); 
				CheckFechaModificacionHomePage = FechaModificacionHomePage;
				setInterval("CompareFechaModificacionHomePage()",180000);
				
				$(document).mousemove(function(e){
						HomePagepageCoords = "( " + e.pageX  + ", " + e.pageY + " )";
						
						if (CheckFechaModificacionHomePage != FechaModificacionHomePage && CheckFechaModificacionHomePage!="" && FechaModificacionHomePage!="" ){
							window.location="/";
						}
					 });
				HomePagepageCoordsActual = HomePagepageCoords;
			}			
			function CompareFechaModificacionHomePage(){
			if (ctrlslideremoltv==0)//control de video emoltv
			{
				if (HomePagepageCoordsActual == "")
					HomePagepageCoordsActual = HomePagepageCoords;
				
				var t=$.getJSON("/_portada/fecha_modificacion_json.asp",function(dato){
				CheckFechaModificacionHomePage = dato.fechahome;				
				if (CheckFechaModificacionHomePage != FechaModificacionHomePage && HomePagepageCoordsActual != HomePagepageCoords){
					window.location="/";
					}							
				}); 
			}
			}	
	
	//]]>   
	</script> 
