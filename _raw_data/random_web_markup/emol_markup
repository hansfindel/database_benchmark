<script type="text/javascript">
		 $(document).ready(function(){
		 	
      busxr = document.getElementById('busxr');
			var whatOptions = ['Restaurantes en Macul', 'Farmacias en Osorno', 'San Diego 480', 'Bancos en Santiago','Clinicas en Parral'];
	    	var whatTimer = 0;
 			var whatTimer = setInterval(function() {
	        	var index = Math.floor( Math.random() * whatOptions.length );
	        	// que.setAttribute('value', whatOptions[index]);
	        	$("#busxr").attr("placeholder", whatOptions[index]);
	            busxr.setAttribute('value', whatOptions[index]);
	    	}, 4000);

        busxr.onfocus = function() {
           busxr.setAttribute('placeholder', '');
           busxr.setAttribute('value', '');
           clearInterval(whatTimer);
        }


 		});

	</script>
