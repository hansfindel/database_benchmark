                suggested: function(element, suggestion) {
                	var email = suggestion.address + "@" + suggestion.domain;
                    var suggestion = "Did you mean " + suggestion.address + "@<b>" + suggestion.domain + "</b>?<br /> " 
                        + "<a href='javascript:void(0);' onclick=$('#email').val('" + email + "');$('#hint').css('display','none');$('#newsletterSignup').submit();>Yes</a> | "
                        + "<a href='javascript:void(0);' onclick=$('#hint').css('display','none');$('#newsletterSignup').submit();>No</a>"
                    $("#hint").html(suggestion).fadeIn(150);
                }
              });
            });
            $("#newsletterSignup").submit(function() {
            	if ($("#hint").is(":visible")) {
            	    return false;
            	}
            });
