<script>
	window.fbAsyncInit = function() {
	    FB.init({
	        appId: '326803741017',
	        status: true,
	        cookie: true,
	        xfbml: true,
	        oauth: true
	    });
	
	    // FB scripts ready. (Does not imply logged in)
	    fb_ready.resolve();
	
	    FB.Event.subscribe('auth.statusChange', function(response) {
	        if(response.status == "connected"){
	            // Log us in to RT internally       
	            $.post("/login/fb_ajax", {
	                fb_id: response.authResponse.userID,
	                fb_access_token: response.authResponse.accessToken,
	                token_expires : response.authResponse.expiresIn
	            }).success( function(flx_response, status) {
	                $(function(){ // DOM ready required for backbone 
	                    RT.LoginStatusModel.set(flx_response); 
	                    console.log("fb_ajax returns. RT.social_init already resolved:" + RT.social_init.isResolved() + " RT.fb_connected already resolved:" + RT.fb_connected.isResolved());
	                
	                    RT.social_init.resolve();
	                    RT.fb_connected.resolve();
	                });
	            });
	              
	        }
	    });
	};


    // Async loading of FB JS SDK
    (function(d, debug){
        var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
        if (d.getElementById(id)) {return;}
        js = d.createElement('script'); js.id = id; js.async = true;
        js.src = "//connect.facebook.net/en_US/all" + (debug ? "/debug" : "") + ".js";
        ref.parentNode.insertBefore(js, ref);
    }(document, /*debug*/ false));
</script>

	<script>console.info("facebook:" + window.performance.now());</script>
	
	
	
	
	
	
	
	
	
		
































    






