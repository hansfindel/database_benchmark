	<script>        
	   console.log("JSONP request for quantcast segments");
	    $.ajax({
	    	  type: 'GET',
	          url: 'http://pixel.quantserve.com/api/segments.json?a=p-0fuRlG_jy3lfA',
	          dataType: 'jsonp',
	    }).done(function( result ){
	        var quantSegs="";
	        for (var i = 0; i < result.segments.length; i++) {
	            quantSegs += result.segments[i].id;
	            if (i<result.segments.length-1) { 
	                quantSegs += ',';
	            }
	        }   
	        setCookie("qcs",quantSegs,90,"/");
	    });
	</script>
 
